{
  "name": "qtum-sync",
  "version": "1.0.0",
  "description": "Qtum sync microservice",
  "main": "dist/main.js",
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@nestjs/mongoose": "^5.0.0",
    "@types/isomorphic-fetch": "0.0.34",
    "dotenv": "^6.0.0",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^5.2.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.1",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/jest": "^23.1.4",
    "@types/nock": "^9.3.0",
    "jest": "^23.3.0",
    "nock": "^9.4.1",
    "ts-jest": "^23.0.0",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^3.4.2",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "build": "rm -rf dist && tsc",
    "lint": "tslint src/**/*.ts",
    "test": "jest"
  },
  "keywords": [
    "qtum",
    "sync",
    "microservice"
  ],
  "author": "@rhrn",
  "license": "BSD"
}
